{
  "expo": {
    "name": "Emu",
    "slug": "Emu",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/emu-bg.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/emu-bg.png",
      "resizeMode": "contain",
      "backgroundColor": "#333"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ios"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/emu-bg.png",
        "backgroundColor": "#333"
      },
      "package": "com.github.calvicii.emu"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/emu-bg.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Outfit-VariableFont.ttf",
            "./assets/fonts/Outfit-Black.ttf",
            "./assets/fonts/Outfit-Bold.ttf",
            "./assets/fonts/Outfit-ExtraBold.ttf",
            "./assets/fonts/Outfit-ExtraLight.ttf",
            "./assets/fonts/Outfit-Light.ttf",
            "./assets/fonts/Outfit-Medium.ttf",
            "./assets/fonts/Outfit-Regular.ttf",
            "./assets/fonts/Outfit-SemiBold.ttf",
            "./assets/fonts/Outfit-Thin.ttf"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e349c6de-2f65-4c21-b4ac-beb220519572"
      }
    }
  }
}
